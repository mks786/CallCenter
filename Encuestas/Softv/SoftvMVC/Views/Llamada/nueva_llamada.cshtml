@{
    Layout = "";

}

<html>

<head>
    <script src="~/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Tell the browser to be responsive to screen width -->
    <!-- Bootstrap 3.3.5 -->
    <title>Nueva Llamada</title>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.css">
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href='https://fonts.googleapis.com/css?family=Arimo' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/plugins/iCheck/square/blue.css">
    <link rel="stylesheet" href="~/plugins/iCheck/square/purple.css">
    <link href="~/plugins/iCheck/square/orange.css" rel="stylesheet" />
    <link href="~/plugins/sweetAlert/sweetalert.css" rel="stylesheet" />
    <script src="~/plugins/sweetAlert/sweetalert.min.js"></script>
    <link href="~/dist/css/bootstrap-switch.css" rel="stylesheet" />
    <style>
        body {
            background-color: #ECF0F5;
        }
    </style>

    <link href="~/dist/js/jquery.flexdatalist.css" rel="stylesheet" />
    <script src="~/dist/js/jquery.flexdatalist.js"></script>
</head>
<body>
    <div class="container" style="background-color:#fff;  box-shadow: 2px 2px 5px #999; margin-top:10px; width:70%;">

        <div class="row">
            <h3 style="padding-left:20px;"><a href="/Llamada" style="color:#5bc0de;"><i class="fa fa-angle-left" aria-hidden="true"></i> Regresar</a></h3>
            <h1 class="text-center" style="color:#4b646f; font-size:22px;"><strong>NUEVA LLAMADA</strong></h1>
            <div class="page-title" style="background-color:#3c8dbc; color:#fff; width:100%;">
                <div class="container">
                    <h3 style="color:#fff; font-size:16px;">
                        
                    </h3>
                </div>
            </div><br />
            <div class="row" id="contenedor_llamda">
                <div class="col-md-12">
                    <p class="text-left" style="padding-left:10px;">
                        <button class="btn btn-warning" onclick="AbrirModal()"><i class="fa fa-map-marker" aria-hidden="true"></i> SELECCIONAR PLAZA</button>
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <h3 class="text-center" style="color:#4b646f;" id="texto_llamda"></h3>
                </div>
            </div>
            <form id="llamadaForm">
                <div class="row" style="padding-left:10px; display:none;" id="tipo_llamante">
                    <div class="col-md-3">
                        <select class="form-control" id="tipo_cliente">
                            <option value="0" selected disabled>¿Quíen llama?</option>
                            <option value="1">Es Cliente</option>
                            <option value="2">No Es Cliente</option>
                        </select>
                    </div>
                    <div class="col-md-5" id="panel_no_cliente" style="display:none;">
                        <input type="text" class="form-control" id="nombre_no_cliente" placeholder="Nombre de la persona que esta llamando" />
                    </div>
                </div><br />
                <div class="row" id="panel_llamante" style="display:none;">
                    <div class="col-md-12">
                        <div class="panel-body">
                            <!-- Nav tabs -->
                            <ul class="nav nav-tabs" role="tablist">
                                <li role="presentation" class="active"><a href="#contrato" aria-controls="contrato" role="tab" data-toggle="tab">Contrato</a></li>
                                <li role="presentation"><a href="#nombre" aria-controls="nombre" role="tab" data-toggle="tab">Nombre</a></li>
                                <li role="presentation"><a href="#direccion" aria-controls="direccion" role="tab" data-toggle="tab">Dirección</a></li>
                            </ul>

                            <!-- Tab panes -->
                            <div class="tab-content">
                                <div role="tabpanel" class="tab-pane active" id="contrato">
                                    <br />
                                    <div class="row">
                                        <div class="col-md-4">
                                            <p style="color:#4b646f;">Ingrese el número de contrato</p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <input type="text" class="form-control" id="input_contrato" />
                                            <span class="help-block">El número de contrato está formado por 2 grupos de números con un guión intermedio p.e. (1234-1)</span>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p class="text-right">
                                                <a class="btn btn-primary" id="buscar_por_contrato"><i class="fa fa-search" aria-hidden="true"></i> Buscar</a>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane" id="direccion">
                                    <br />
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="ciudad_busqueda" placeholder="Ciudad" />
                                            </div>
                                        </div><br />
                                        <div class="row">
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="colonia_domicilio" placeholder="Colonia" />
                                            </div>
                                        </div><br />
                                        <div class="row">
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="calle_busqueda" placeholder="Calle" />
                                            </div>
                                        </div><br />
                                        <div class="row">
                                            <div class="col-md-4">
                                                <input type="text" class="form-control" id="numero_domicilio" placeholder="Número" />
                                            </div>
                                        </div>
                                    </div><br />
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p class="text-right">
                                                <a class="btn btn-primary" id="buscar_por_direccion"><i class="fa fa-search" aria-hidden="true"></i> Buscar</a>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div role="tabpanel" class="tab-pane" id="nombre">
                                    <br />
                                    <div class="row">
                                        <div class="col-md-4">
                                            <p style="color:#4b646f;">Nombre completo</p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <input type="text" class="form-control" id="nombre_individual" />
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p class="text-right">
                                                <a class="btn btn-primary" id="buscar_por_nombre"><i class="fa fa-search" aria-hidden="true"></i> Buscar</a>
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div><!-- panel body -->
                    </div>
                </div>
                <div class="row" style="display:none;" id="panel_tabla_clientes">
                    <div class="col-md-12 container">
                        <table id="Tabla_Clientes" class="table table-bordered table-hover dataTable">
                            <thead>
                                <tr>
                                    <td><b>Contrato</b></td>
                                    <td><b>Nombre</b> </td>
                                    <td><b>Ciudad</b></td>
                                    <td><b>Colonia</b></td>
                                    <td><b>Calle</b></td>
                                    <td><b>Número</b></td>
                                    <td><b>Opciones</b></td>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_datos_cliente">
                    <div class="col-md-12">
                        <h4 id="texto_contrato" style="color:#4b646f;"></h4>
                        <input type="hidden" id="contrato_oculto" />
                        <h4 id="texto_nombre" style="color:#4b646f;"></h4>
                        <h5 id="texto_colonia" style="color:#4b646f;"></h5>
                        <h5 id="texto_calle" style="color:#4b646f;"></h5>
                        <h5 id="texto_numero" style="color:#4b646f;"></h5>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_fecha">
                    <div class="col-md-2">
                        <label style="color:#4b646f;">Hora Inicial</label>
                        <input type="time" id="hora_inicio" class="form-control">
                    </div>
                    <div class="col-md-3">
                        <label style="color:#4b646f;">Fecha</label>
                        <input type="date" id="fecha_llamada" class="form-control" />
                    </div>
                </div><br />
                <div class="row" id="motivo_no_cliente" style="padding-left:10px; display:none;">
                    <div class="col-md-6">
                        <label style="color:#4b646f;">Motivo de la llamada</label>
                        <textarea class="form-control" id="motivo"></textarea>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="motivo_llamada_panel">
                    <div class="col-md-3" style="padding-top:10px;">
                        <label style="color:#4b646f;">Tipo de Servicio</label>
                        <select class="form-control" id="tipo_servicio">
                            <option value="0" selected disabled>Tipo de servicio</option>
                        </select>
                    </div>
                </div><br />
                <div class="row" id="solucion_no_cliente" style="padding-left:10px; display:none;">
                    <div class="col-md-6">
                        <label style="color:#4b646f;">Solución</label>
                        <textarea class="form-control" id="solucion"></textarea>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_clasificacion">
                    <div class="col-md-4">
                        <label style="color:#4b646f;">Clasificación del problema</label>
                        <select class="form-control" id="select_problemas">
                            <option value="0" selected disabled>Clasificación del problema</option>
                        </select>
                    </div>
                </div><br />
                <div class="row" style="display:none; padding-left:10px;" id="panel_solucion">
                    <div class="col-md-4">
                        <label style="color:#4b646f;">Clasificación de la solución</label>
                        <select class="form-control" id="select_soluciones">
                            <option value="0" selected disabled>Clasificación de la solución</option>
                        </select>
                    </div>
                    <div class="col-md-2" style="height:25px; display:none;" id="panel_queja">
                        <label style="color:#4b646f;">Realizar Queja</label>
                        <input type="checkbox" name="levantar_queja" id="queja">
                    </div>
                </div><br />
                <div class="row" style="display:none; padding-left:10px;" id="panel_nueva_queja">
                    <fieldset>
                        <legend style="padding-left:25px; font-size:12px; color:#4b646f;">Realizar Queja</legend>
                        <div class="col-md-3">
                            <label style="color:#4b646f;">Departamento Responsable</label>
                            <select class="form-control" id="departamento">
                                <option value="0" selected disabled>Departamento Responsable</option>
                            </select>
                        </div>
                        <div class="col-md-3">
                            <label style="color:#4b646f;">Tipo Prioridad</label>
                            <select class="form-control" id="prioridad">
                                <option value="0" selected disabled>Tipo Prioridad</option>
                            </select>
                        </div>
                    </fieldset>
                </div><br />
                <div class="row" style="display:none; padding-left:10px;" id="trabajo_panel">
                    <div class="col-md-5">
                        <label style="color:#4b646f;">Trabajo</label>
                        <select class="form-control" id="trabajo">
                            <option value="0" selected disabled>Trabajo</option>
                        </select>
                    </div>
                    <div class="col-md-3">
                        <label style="color:#4b646f;">Turno</label>
                        <select class="form-control" id="turno">
                            <option value="0" selected disabled>Turno</option>
                        </select>
                    </div>
                </div>
            </form>
            <div class="row" style="padding-right:40px; display:none;" id="btn_enviar">
                <div class="col-md-12">
                    <p class="text-right">
                        <button class="btn btn-primary" onclick="guardarLlamada()"> Guaradar Llamada</button>
                        <button onclick="reset()">Reset</button>
                    </p>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="Modal_Plaza" role="dialog">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <button class="close" aria-label="Close" type="button" data-dismiss="modal"><span aria-hidden="true">×</span></button>
                    <h4 class="modal-title">Selecciona la plaza</h4>
                </div>
                <div class="modal-body">
                    <div class="col-md-6 col-md-offset-3">
                        <select class="form-control" id="conectando_plaza">
                            <option value="default" selected disabled>Selecciona la plaza</option>
                        </select>
                    </div>
                    <br />
                    <br />
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary " type="button" data-dismiss="modal">Cerrar</button>
                    <button class="btn btn-success" onclick="plaza_conexion()" type="button" id="aceptar_plaza">Conectar</button>
                </div>
            </div>
        </div>
    </div>
</body>
<script type="text/javascript" src="~/dist/js/bootstrap-switch.js"></script>
<script type="text/javascript">
    var count_queja = 2;
    $(function () {
        $("[name='levantar_queja']").bootstrapSwitch({
            onText: 'SI',
            offText: 'NO',
            offColor: 'danger',
            onColor: 'success'
        });
        $.ajax({
            url: "/Conexion/ListaConexiones/",
            type: "GET",
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#conectando_plaza').append($('<option>', {
                        value: data[i].IdConexion,
                        text: data[i].Plaza
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });

        $('input[name="levantar_queja"]').on('switchChange.bootstrapSwitch', function (event, state) {
            if (state) {
                $('#panel_nueva_queja').show();
                $('#trabajo_panel').show();
                count_queja = 1;
            } else {
                $('#panel_nueva_queja').hide();
                $('#trabajo_panel').hide();
                count_queja = 2;
            }
        });
    });


    function tipoServicio(id) {
        $.ajax({
            url: "/CLIENTE/getTipoServicio/",
            type: "GET",
            data: { "IdPlaza": id },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#tipo_servicio').append($('<option>', {
                        value: data[i].Clv_TipSer,
                        text: data[i].Concepto
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
    }

    function AbrirModal() {
        $('#Modal_Plaza').modal("show");
    }


    function plaza_conexion() {
        $('#contenedor_llamda').hide();
        $('#llamdaForm').show();
        $('#texto_llamda').text('Plaza ' + $("#conectando_plaza option:selected").text());
        $('#Modal_Plaza').modal("hide");
        var id_plaza = $('#conectando_plaza').val();
        tipoServicio(id_plaza);
        $('#tipo_llamante').show();
        $('#panel_cliente').show();
        $.ajax({
            url: "/tblClasificacionProblema/GetClasficacionProblema/",
            type: "GET",
            data: { 'IdPlaza': id_plaza },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#select_problemas').append($('<option>', {
                        value: data[i].clvProblema,
                        text: data[i].descripcion
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
        $.ajax({
            url: "/tblClasificacionProblema/GetClasficacionSolucion/",
            type: "GET",
            data: { 'IdPlaza': id_plaza },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#select_soluciones').append($('<option>', {
                        value: data[i].CLV_TRABAJO,
                        text: data[i].DESCRIPCION
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
        $.ajax({
            url: "/tblClasificacionProblema/GetDepartamentoResponsable/",
            type: "GET",
            data: { 'IdPlaza': id_plaza },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#departamento').append($('<option>', {
                        value: data[i].Clave,
                        text: data[i].Concepto
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
        $.ajax({
            url: "/tblClasificacionProblema/GetPrioridad/",
            type: "GET",
            data: { 'IdPlaza': id_plaza },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#prioridad').append($('<option>', {
                        value: data[i].clvPrioridadQueja,
                        text: data[i].Descripcion
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
        $.ajax({
            url: "/tblClasificacionProblema/GetTurno/",
            type: "GET",
            data: { 'IdPlaza': id_plaza },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#turno').append($('<option>', {
                        value: data[i].IdTurno,
                        text: data[i].Turno
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
    }

    $("#tipo_cliente").change(function () {
        var tipo = $("#tipo_cliente").val();
        if (tipo == 1) {
            $('#btn_enviar').hide();
            $('#panel_no_cliente').hide();
            $('#panel_llamante').show();
            $('#motivo_llamada_panel').hide();
            $('#queja').hide();
            $('#tipo_servicio').hide();
            $('#panel_fecha').hide();
            $('#panel_clasificacion').hide();
            $('#panel_solucion').hide();
            $('#panel_nueva_queja').hide();
            $('#motivo_no_cliente').hide();
            $('#solucion_no_cliente').hide();
            //valores a default
        } else if (tipo == 2) {
            $('#panel_no_cliente').show();
            $('#panel_llamante').hide();
            $('#panel_tabla_clientes').hide();
            $('#panel_datos_cliente').hide();
            $('#motivo_llamada_panel').hide();
            $('#queja').hide();
            $('#tipo_servicio').hide();
            $('#panel_queja').hide();
            $('#motivo_no_cliente').show();
            $('#panel_clasificacion').hide();
            $('#trabajo_panel').hide();
            var now = new Date();
            var hour = now.getHours();
            if (hour < 10) {
                hour = "0" + hour;
            }
            var minute = now.getMinutes();
            if (minute < 10) {
                minute = "0" + minute;
            }
            $('#hora_inicio').val(hour + ":" + minute);
            document.getElementById('fecha_llamada').valueAsDate = new Date();
            $('#panel_fecha').show();
            $('#panel_solucion').hide();
            $('#panel_nueva_queja').hide();
            $('#solucion_no_cliente').show();
            $('#btn_enviar').show();
        }

    });


    $('#buscar_por_nombre').on('click', function () {
        var id_plaza = $('#conectando_plaza').val();
        var nombre = $('#nombre_individual').val();
        $('#panel_masivo').hide();
        $('#panel_individual').show();
        $.ajax({
            url: "/CLIENTE/GetClientesPRUEBA/",
            type: "GET",
            data: { 'IdPlaza': id_plaza, "contrato": "", "Nombrecliente": nombre, "direccion": "" },
            success: function (data, textStatus, jqXHR) {
                if (data.length == 0) {
                    $('#panel_tabla_clientes').hide();
                    $('#invalido').show();
                } else {
                    $('#invalido').hide();
                    $('#panel_tabla_clientes').show();
                    $('#Tabla_Clientes tbody > tr').remove();
                    for (var i = 0; i < data.length; i++) {
                        $('#Tabla_Clientes tbody').append('<tr><td>' + data[i].CONTRATO + '</td><td>' + data[i].NOMBRE + '</td><td>' + data[i].Ciudad + '</td><td>' + data[i].Colonia + '</td><td>' + data[i].Calle + '</td><td>' + data[i].NUMERO + '</td><td><a id="' + data[i].CONTRATO + '" data-name="' + data[i].NOMBRE + '" data-colonia="' + data[i].Colonia + '" data-calle="' + data[i].Calle + '" data-numero="' + data[i].NUMERO + '" class="btn btn-success" onclick="selecion_cliente(this)">Seleccionar</a></td></tr>');

                    }
                }
            },
            error: function (data, jqXHR, textStatus) {
                console.log(data);
            }

        });
    });

    $('#buscar_por_direccion').on('click', function () {
        var id_plaza = $('#conectando_plaza').val();
        var calle = $('#calle_busqueda').val();
        var ciudad = $('#ciudad_busqueda').val();
        var numero = $('#numero_domicilio').val();
        var colonia = $('#colonia_domicilio').val();
        $('#panel_masivo').hide();
        $('#panel_individual').show();
        $.ajax({
            url: "/CLIENTE/GetClientesPRUEBA/",
            type: "GET",
            data: { 'IdPlaza': id_plaza, "contrato": "", "Nombrecliente": "", "calle": calle, "colonia": colonia, "ciudad": ciudad, "numero": numero },
            success: function (data, textStatus, jqXHR) {
                if (data.length == 0) {
                    $('#panel_tabla_clientes').hide();
                    $('#invalido').show();
                } else {
                    $('#invalido').hide();
                    $('#panel_tabla_clientes').show();
                    $('#Tabla_Clientes tbody > tr').remove();
                    for (var i = 0; i < data.length; i++) {
                        $('#Tabla_Clientes tbody').append('<tr><td>' + data[i].CONTRATO + '</td><td>' + data[i].NOMBRE + '</td><td>' + data[i].Ciudad + '</td><td>' + data[i].Colonia + '</td><td>' + data[i].Calle + '</td><td>' + data[i].NUMERO + '</td><td><a id="' + data[i].CONTRATO + '" data-name="' + data[i].NOMBRE + '" data-colonia="' + data[i].Colonia + '" data-calle="' + data[i].Calle + '" data-numero="' + data[i].NUMERO + '" class="btn btn-success" onclick="selecion_cliente(this)">Seleccionar</a></td></tr>');

                    }
                }
            },
            error: function (data, jqXHR, textStatus) {
                console.log(data);
            }

        });
    });

    $('#buscar_por_contrato').on('click', function () {
        var id_plaza = $('#conectando_plaza').val();
        var contrato = $('#input_contrato').val();
        $('#panel_masivo').hide();
        $('#panel_individual').show();
        $.ajax({
            url: "/CLIENTE/GetClientesPRUEBA/",
            type: "GET",
            data: { 'IdPlaza': id_plaza, "contrato": contrato, "cliente1": "", "direccion": "" },
            success: function (data, textStatus, jqXHR) {
                if (data.length == 0) {
                    $('#panel_tabla_clientes').hide();
                    $('#invalido').show();
                } else {
                    $('#invalido').hide();
                    $('#panel_tabla_clientes').show();
                    $('#Tabla_Clientes tbody > tr').remove();
                    $('#Tabla_Clientes tbody').append('<tr><td>' + data[0].CONTRATO + '</td><td>' + data[0].NOMBRE + '</td><td>' + data[0].Ciudad + '</td><td>' + data[0].Colonia + '</td><td>' + data[0].Calle + '</td><td>' + data[0].NUMERO + '</td><td><a id="' + data[0].CONTRATO + '" data-name="' + data[0].NOMBRE + '" data-colonia="' + data[0].Colonia + '" data-calle="' + data[0].Calle + '" data-numero="' + data[0].NUMERO + '" class="btn btn-success" onclick="selecion_cliente(this)">Seleccionar</a></td></tr>');
                }
            },
            error: function (data, jqXHR, textStatus) {
                console.log(data);
            }

        });
    });

    $("#tipo_servicio").change(function () {
        var idServicio = $("#tipo_servicio").val();
        var id_plaza = $('#conectando_plaza').val();
        $.ajax({
            url: "/Llamada/GetIdTrabajo/",
            type: "GET",
            data: { 'IdPlaza': id_plaza, "idServicio": idServicio },
            success: function (data, textStatus, jqXHR) {
                $('#trabajo').find('option').remove().end();
                for (var i = 0; i < data.length; i++) {
                    $('#trabajo').append($('<option>', {
                        value: data[i].Clv_Trabajo,
                        text: data[i].Descripcion
                    }));
                }
            },
            error: function (data, jqXHR, textStatus) {
                console.log(data);
            }
        });
    });


    function selecion_cliente(e) {
        $('#contrato_oculto').val(e.getAttribute('id'));
        $('#texto_contrato').text("No. contrato: " + e.getAttribute('id'));
        $('#texto_nombre').text("Nombre: " + e.getAttribute('data-name'));
        $('#texto_colonia').text("Colonia: " + e.getAttribute('data-colonia'));
        $('#texto_calle').text("Calle: " + e.getAttribute('data-calle'));
        $('#texto_numero').text("Número: " + e.getAttribute('data-numero'));
        $('#panel_tabla_clientes').hide();
        $('#panel_llamante').hide();
        $('#panel_datos_cliente').show();
        $('#queja').show();
        $('#motivo_llamada_panel').show();
        $('#tipo_servicio').show();
        $('#motivo_no_cliente').show();
        $('#solucion_no_cliente').show();
        var now = new Date();
        var hour = now.getHours();
        if (hour < 10) {
            hour = "0" + hour;
        }
        var minute = now.getMinutes();
        if (minute < 10) {
            minute = "0" + minute;
        }
        $('#hora_inicio').val(hour + ":" + minute);
        document.getElementById('fecha_llamada').valueAsDate = new Date();
        $('#panel_fecha').show();
        $('#panel_clasificacion').show();
        $('#panel_solucion').show();
        $('#panel_queja').show();
        $('#btn_enviar').show();

    }


    function guardarLlamada(){
        var tipo = $("#tipo_cliente").val();
        var llamada = {};
        if (tipo == 1) {
            llamada.tipo_llamada = tipo;
            llamada.IdConexion = $('#conectando_plaza').val();
            llamada.usuario = @Session["idusuario"];
            llamada.contrato = $('#contrato_oculto').val();
            llamada.fecha = $('#fecha_llamada').val()+" "+$('#hora_inicio').val();
            llamada.horainicio = $('#fecha_llamada').val()+" "+$('#hora_inicio').val();
            llamada.horatermino = $('#fecha_llamada').val()+" "+$('#hora_inicio').val();
            if($('#motivo_no_cliente').val() == ""){
                console.log();
                swal("Por favor introduce el motivo de la llamada", "", "error");
            }
            else{
                llamada.motivo = $('#motivo_no_cliente').val();
                if ($('#tipo_servicio').val() == null) {
                    swal("Por favor selecciona el tipo de servicio", "", "error");
                } else {
                    llamada.Clv_TipSer = $('#tipo_servicio').val();
                    if($('#solucion_no_cliente').show() == ""){
                        swal("Por favor introduce la solución", "", "error");
                    }else{
                        llamada.solucion = $('#solucion_no_cliente').val();
                        if ($('#select_problemas').val() == null) {
                            swal("Por favor selecciona la clasificación del problema", "", "error");
                        } else {
                            llamada.clas_problema = $('#select_problemas').text();
                            if ($('#select_soluciones').val() == null) {
                                swal("Por favor selecciona la clasificación de la solución", "", "error");
                            } else {
                                llamada.clas_solucion = $('#select_soluciones').text();
                                llamada.queja = count_queja;
                                if (count_queja == 2) {
                                    var id_plaza = $('#conectando_plaza').val();
                                    $.ajax({
                                        url: "/Llamada/InsertLlamada",
                                        type: "POST",
                                        data: { 'IdPlaza': id_plaza, "llamada": llamada},
                                        success: function (data, textStatus, jqXHR) {
                                            $('#panel_llamante').hide();
                                            $('#panel_datos_cliente').hide();
                                            $('#panel_fecha').hide();
                                            $('#motivo_llamada_panel').hide();
                                            $('#panel_clasificacion').hide();
                                            $('#panel_solucion').hide();
                                            $('#panel_queja').hide();
                                            $('#panel_nueva_queja').hide();
                                            $('#btn_enviar').hide();
                                            document.getElementById("llamadaForm").reset();
                                            swal("La llamada se guaró exitosamente", "", "success");
                                        },
                                        error: function (data, jqXHR, textStatus) {
                                            console.log(data);
                                        }

                                    });
                            
                                } else if (count_queja == 1) {
                                    if($('#departamento').val() == null){
                                        swal("Por favor selecciona el departamento responsable", "", "error");
                                    } else {
                                        llamada.dep_responsable = $('#departamento').val();
                                        if ($('#prioridad').val() == null) {
                                            swal("Por favor selecciona la prioridad de la queja", "", "error");
                                        } else {
                                            llamada.prioridad = $('#prioridad').val();
                                            var id_plaza = $('#conectando_plaza').val();
                                            $.ajax({
                                                url: "/Llamada/InsertLlamada",
                                                type: "POST",
                                                data: { 'IdPlaza': id_plaza, "llamada": llamada},
                                                success: function (data, textStatus, jqXHR) {
                                                    $('#panel_llamante').hide();
                                                    $('#panel_datos_cliente').hide();
                                                    $('#panel_fecha').hide();
                                                    $('#motivo_llamada_panel').hide();
                                                    $('#panel_clasificacion').hide();
                                                    $('#panel_solucion').hide();
                                                    $('#panel_queja').hide();
                                                    $('#panel_nueva_queja').hide();
                                                    $('#btn_enviar').hide();
                                                    document.getElementById("llamadaForm").reset();
                                                    $('#btn_enviar').hide();
                                                    swal("La llamada se guaró exitosamente", "", "success");
                                                },
                                                error: function (data, jqXHR, textStatus) {
                                                    console.log(data);
                                                }

                                            });

                                        }
                                    }
                                }
                            }
                        }
                    }
                    
                }
            }
            

        } else if (tipo == 2) {
            if ($('#nombre_no_cliente').val() == "") {
                swal("Por favor ingresa el nombre de la persona que habla", "", "error");
            } else {
                llamada.nombre = $('#nombre_no_cliente').val();
                if($('#motivo').val() == "") {
                    swal("Por favor ingresa el motivo de la llamda", "", "error");
                }else{
                    llamada.clas_solucion = $('#select_soluciones').val();
                    alert('ajax para no clientes');
                    $('#panel_no_cliente').hide();
                    $('#panel_fecha').hide();
                    $('#btn_enviar').hide();
                    $('#motivo_no_cliente').hide();
                    document.getElementById("llamadaForm").reset();
                }
            }
        }
    }

</script>

</html>





