@{
    Layout = "";

}

<html>

<head>
    <script src="~/plugins/jQuery/jQuery-2.1.4.min.js"></script>
    <!-- Tell the browser to be responsive to screen width -->
    <!-- Bootstrap 3.3.5 -->
    <title>Nueva Llamada</title>
    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.css">
    <script src="~/bootstrap/js/bootstrap.min.js"></script>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href='https://fonts.googleapis.com/css?family=Arimo' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="~/plugins/iCheck/square/blue.css">
    <link rel="stylesheet" href="~/plugins/iCheck/square/purple.css">
    <link href="~/plugins/iCheck/square/orange.css" rel="stylesheet" />
    <link href="~/plugins/sweetAlert/sweetalert.css" rel="stylesheet" />
    <script src="~/plugins/sweetAlert/sweetalert.min.js"></script>
    <link href="~/dist/css/bootstrap-switch.css" rel="stylesheet" />
    <style>
        body {
            background-color: #ECF0F5;
        }
        .scroll_div{
             overflow: scroll;
        }
         input:required:invalid, input:focus:invalid {
            background-image: url(data:image/png;base64,);
            background-position: right top;
            background-repeat: no-repeat;
            -moz-box-shadow: none;
          }
          input:required:valid {
            background-image: url(data:image/png;base64,);
            background-position: right top;
            background-repeat: no-repeat;
          }
    </style>

    <link href="~/dist/js/jquery.flexdatalist.css" rel="stylesheet" />
    <script src="~/dist/js/jquery.flexdatalist.js"></script>
</head>
<body>
    <div class="container" style="background-color:#fff;  box-shadow: 2px 2px 5px #999; margin-top:10px; width:70%;">

        <div class="row">
            <h3 style="padding-left:20px;"><a href="/Llamada" style="color:#5bc0de;"><i class="fa fa-angle-left" aria-hidden="true"></i> Menú Principal</a></h3>
            <h1 class="text-center" style="color:#4b646f; font-size:22px;"><strong>NUEVA LLAMADA</strong></h1>
            <div class="page-title" style="background-color:#3c8dbc; color:#fff; width:100%;">
                <div class="container">
                    <h3 style="color:#fff; font-size:16px;">
                        
                    </h3>
                </div>
            </div><br />
            <div class="row">
                <div class="col-md-12">
                    <h3 class="text-center" style="color:#4b646f;" id="texto_llamda"></h3>
                </div>
            </div>
            <form id="llamadaForm">
                <div class="row" style="padding-left:10px; display:none;" id="tipo_llamante">
                    <div class="col-md-3">
                        <label style="color:#4b646f;">¿Es cliente?</label>
                        <select class="form-control" id="tipo_cliente">
                            <option value="0" selected disabled>---------</option>
                            <option value="1">Si</option>
                            <option value="2">No</option>
                        </select>
                    </div>
                </div><br />
                <div class="row" style="padding-left:10px; display:none;" id="regresar">
                    <div class="col-md-3">
                        <h4><a href="#" style="color:#5bc0de;" onclick="regresar()"><i class="fa fa-angle-left" aria-hidden="true"></i> Regresar</a></h4>
                    </div>
                </div>
                <div class="row" id="panel_no_cliente" style="padding-left:10px; display:none;">
                    <div class="col-md-5">
                        <label style="color:#4b646f; font-size:12px;">Nombre</label>
                        <input type="text" class="form-control" id="nombre_no_cliente" placeholder="Nombre de la persona que esta llamando" />
                    </div>
                </div>
                <div class="row" id="panel_llamante" style="display:none;">
                    <div class="col-md-12">
                        <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="headingOne">
                                    <h4 class="panel-title">
                                        <a role="button" id="busqueda_collapse" data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne" style="color:#4b646f;">
                                            Busqueda individual
                                        </a>
                                    </h4>
                                </div>
                                <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
                                    <div class="panel-body">
                                        <!-- Nav tabs -->
                                        <ul class="nav nav-tabs" role="tablist">
                                            <li role="presentation" class="active"><a href="#contrato" aria-controls="contrato" role="tab" data-toggle="tab">Contrato</a></li>
                                            <li role="presentation"><a href="#nombre" aria-controls="nombre" role="tab" data-toggle="tab">Nombre</a></li>
                                            <li role="presentation"><a href="#direccion" aria-controls="direccion" role="tab" data-toggle="tab">Dirección</a></li>
                                        </ul>

                                        <!-- Tab panes -->
                                        <div class="tab-content">
                                            <div role="tabpanel" class="tab-pane active" id="contrato">
                                                <br />
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <p style="color:#4b646f;">Ingrese el número de contrato</p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-3">
                                                        <input type="text" class="form-control" id="input_contrato" />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-8">
                                                        <span class="help-block">El número de contrato está formado por 2 grupos de números con un guión intermedio p.e. (1234-1)</span>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <p class="text-right">
                                                            <a class="btn btn-primary" id="buscar_por_contrato"><i class="fa fa-search" aria-hidden="true"></i> Buscar</a>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div role="tabpanel" class="tab-pane" id="direccion">
                                                <br />
                                                <div class="container">
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" id="ciudad_busqueda" placeholder="Ciudad" />
                                                        </div>
                                                    </div><br />
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" id="colonia_domicilio" placeholder="Colonia" />
                                                        </div>
                                                    </div><br />
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" id="calle_busqueda" placeholder="Calle" />
                                                        </div>
                                                    </div><br />
                                                    <div class="row">
                                                        <div class="col-md-4">
                                                            <input type="text" class="form-control" id="numero_domicilio" placeholder="Número" />
                                                        </div>
                                                    </div>
                                                </div><br />
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <p class="text-right">
                                                            <a class="btn btn-primary" id="buscar_por_direccion"><i class="fa fa-search" aria-hidden="true"></i> Buscar</a>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                            <div role="tabpanel" class="tab-pane" id="nombre">
                                                <br />
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <p style="color:#4b646f;">Nombre completo</p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-8">
                                                        <input type="text" class="form-control" id="nombre_individual" />
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <p class="text-right">
                                                            <a class="btn btn-primary" id="buscar_por_nombre"><i class="fa fa-search" aria-hidden="true"></i> Buscar</a>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div><!-- panel body -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row" id="invalido" style="display:none;">
                    <div class="col-md-12">
                        <div class="alert alert-warning alert-dismissible fade in" role="alert"><strong>Opsss!</strong> No se encontraron coincidencias. </div>
                    </div>
                </div>
                <div class="row" style="display:none;" id="panel_tabla_clientes">
                    <div class="col-md-12 container">
                        <table id="Tabla_Clientes" class="table table-bordered table-hover dataTable">
                            <thead style="font-size:12px;">
                                <tr>
                                    <td><b>Contrato</b></td>
                                    <td><b>Nombre</b> </td>
                                    <td><b>Ciudad</b></td>
                                    <td><b>Colonia</b></td>
                                    <td><b>Calle</b></td>
                                    <td><b>Número</b></td>
                                    <td><b>Opciones</b></td>
                                </tr>
                            </thead>
                            <tbody style="font-size:12px;"></tbody>
                        </table>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_datos_cliente">
                    <div class="col-md-6">
                        <h5 id="texto_contrato" style="color:#4b646f;"></h5>
                        <input type="hidden" id="contrato_oculto" />
                        <h5 id="texto_nombre" style="color:#4b646f;"></h5>
                        <h5 id="texto_colonia" style="color:#4b646f;"></h5>
                        <h5 id="texto_calle" style="color:#4b646f;"></h5>
                        <h5 id="texto_numero" style="color:#4b646f;"></h5>
                    </div>
                    <div class="col-md-5">
                        <div id="servicios_contratados">

                        </div>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_tipo_llamada_cliente">
                    <div class="col-md-3">
                        <label style="color:#4b646f; font-size:12px;">Selecciona el tipo de llamada</label>
                        <select class="form-control" id="tipo_llamada_cliente">
                            <option value="0" selected disabled>----------------</option>
                            <option value="1">Información</option>
                            <option value="2">Tiene un problema</option>    
                        </select>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_tipo_informacion">
                    <div class="col-md-5">
                        <label style="color:#4b646f; font-size:12px;">Selecciona el tipo de información</label>
                        <select class="form-control" id="tipo_informacion">
                            <option value="0" selected disabled>----------------</option>
                        </select>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="telefono_no_cliente">
                    <div class="col-md-3">
                        <label style="color:#4b646f; font-size:12px;">Teléfono</label>
                        <input type="text" id="telefono" class="form-control">
                    </div>
                    <div class="col-md-3">
                        <label style="color:#4b646f; font-size:12px;">Celular</label>
                        <input type="text" id="celular" class="form-control">
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="email_no_cliente">
                    <div class="col-md-4">
                        <label style="color:#4b646f; font-size:12px;">Email</label>
                        <input type="email" id="email" class="form-control" required>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;">
                    <div class="col-md-2">
                        <label style="color:#4b646f; font-size:12px;">Hora Inicial</label>
                        <input type="time" id="hora_inicio" class="form-control">
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="domicilio_no_cliente">
                    <div class="col-md-6">
                        <label style="color:#4b646f; font-size:12px;">Domicilio</label>
                        <textarea type="text" id="domicilio" class="form-control"></textarea>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="motivo_llamada_panel">
                    <div class="col-md-3" style="padding-top:10px;">
                        <label style="color:#4b646f; font-size:12px;">Tipo de Servicio</label>
                        <select class="form-control" id="tipo_servicio">
                            <option value="0" selected disabled>Tipo de servicio</option>
                        </select>
                    </div>
                </div>
                <div class="row" id="motivo_llamada_cliente" style="padding-left:10px; display:none;">
                    <div class="col-md-6">
                        <label style="color:#4b646f; font-size:12px;">Motivo Llamada</label>
                        <select class="form-control" id="motivo_de_llamada">
                            <option value="0" selected disabled>Motivo Llamada</option>
                        </select>
                    </div>
                </div>
                <div class="row" id="motivo_no_cliente" style="padding-left:10px; display:none;">
                    <div class="col-md-6">
                        <label style="color:#4b646f; font-size:12px;">Detalle</label>
                        <textarea class="form-control" id="motivo"></textarea>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_clasificacion">
                    <div class="col-md-4">
                        <label style="color:#4b646f; font-size:12px;">Clasificación del problema</label>
                        <select class="form-control" id="select_problemas">
                            <option value="0" selected disabled>Clasificación del problema</option>
                        </select>
                    </div>
                </div>
                <div class="row" id="solucion_no_cliente" style="padding-left:10px; display:none;">
                    <div class="col-md-6">
                        <label style="color:#4b646f; font-size:12px;">Solución</label>
                        <textarea class="form-control" id="solucion"></textarea>
                    </div>
                </div>
                <div class="row" style="display:none; padding-left:10px;" id="panel_solucion">
                    <div class="col-md-4">
                        <label style="color:#4b646f; font-size:12px;">Clasificación de la solución</label>
                        <select class="form-control" id="select_soluciones">
                            <option value="0" selected disabled>Clasificación de la solución</option>
                        </select>
                    </div>
                    <div class="col-md-2" style="height:25px; display:none;" id="panel_queja">
                        <label style="color:#4b646f; font-size:12px;">Realizar Queja</label>
                        <input type="checkbox" name="levantar_queja" id="queja">
                    </div>
                </div><br />
                <div class="row" style="display:none; padding-left:10px;" id="panel_nueva_queja">
                    <fieldset>
                        <legend style="padding-left:25px; font-size:12px; color:#4b646f;">Realizar Queja</legend>
                        <div class="col-md-4">
                            <label style="color:#4b646f; font-size:12px;">Departamento Responsable</label>
                            <select class="form-control" id="departamento">
                                <option value="0" selected disabled>Departamento Responsable</option>
                            </select>
                        </div>
                        <div class="col-md-3">
                            <label style="color:#4b646f; font-size:12px;">Tipo Prioridad</label>
                            <select class="form-control" id="prioridad">
                                <option value="0" selected disabled>Tipo Prioridad</option>
                            </select>
                        </div>
                    </fieldset>
                </div><br />
                <div class="row" style="display:none; padding-left:10px;" id="trabajo_panel">
                    <div class="col-md-3">
                        <label style="color:#4b646f; font-size:12px;">Turno</label>
                        <select class="form-control" id="turno">
                            <option value="0" selected disabled>Turno</option>
                        </select>
                    </div>
                </div>
            </form>
            <div class="row" style="padding-right:40px; display:none;" id="btn_enviar">
                <div class="col-md-12">
                    <p class="text-right">
                        <button class="btn btn-primary" onclick="guardarLlamada()"> Guaradar Llamada</button>
                    </p>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="Modal_Plaza" role="dialog">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="modal-header">
                    <button class="close" aria-label="Close" type="button" data-dismiss="modal"><span aria-hidden="true">×</span></button>
                </div>
                <div class="modal-body">
                    <div class="col-md-12">
                        <div class="form-group">
                            <p class="col-md-4 control-label" for="selectbasic" style="padding-top:10px; color:#4b646f;">Selecciona la plaza</p>
                            <div class="col-md-6">
                                <select id="conectando_plaza" class="form-control"></select>
                            </div>
                        </div>
                    </div>
                    <br />
                    <br />
                </div>
                <div class="modal-footer">
                    <button class="btn btn-primary " type="button" data-dismiss="modal">Cerrar</button>
                    <button class="btn btn-success" onclick="plaza_conexion()" type="button" id="aceptar_plaza">Conectar</button>
                </div>
            </div>
        </div>
    </div>
</body>
<script src="~/plugins/slimScroll/jquery.slimscroll.js"></script>
<script src="~/plugins/input-mask/jquery.inputmask.js"></script>
<script type="text/javascript" src="~/dist/js/bootstrap-switch.js"></script>
<script type="text/javascript">
    $(function(){
        $('#servicios_contratados').slimScroll({
            height: '200px'
        });
    });
    var count_queja = 2;
    var id_plaza;
    $(function () {
        //tomamos el valor de la variable que viene por url
        id_plaza = location.search.split('id_plaza=')[1];
        //si la plaza es nula redirigios al menu de llamadas
        if(id_plaza == ""){
            location.href='/Llamada';
        }else{
            //si la variable no es nula activamos las funciones mandandoles el id de la palza
            tipoServicio(id_plaza);
            plaza_conexion(id_plaza);
        }
        $("[name='levantar_queja']").bootstrapSwitch({
            onText: 'SI',
            offText: 'NO',
            offColor: 'danger',
            onColor: 'success'
        });
        $('#telefono').inputmask("(999)9999999");
        $('#celular').inputmask("999-999-99-99");


        $('input[name="levantar_queja"]').on('switchChange.bootstrapSwitch', function (event, state) {
            if (state) {
                $('#panel_nueva_queja').show();
                $('#trabajo_panel').show();
                count_queja = 1;
            } else {
                $('#panel_nueva_queja').hide();
                $('#trabajo_panel').hide();
                count_queja = 2;
            }
        });
        $('#tipo_servicio').on('change', function(){
            var id = $(this).val();
            getSolucuionLlenar(id);
        });
        $.ajax({
            url: "/Llamada/GetMotivoLlamada/",
            type: "GET",
            data: { "IdPlaza": id_plaza },
            success: function (data, textStatus, jqXHR) {
                // una vez que fue exitosa la peticion asignamos los datos a un select
                for (var i = 0; i < data.length; i++) {
                    $('#motivo_de_llamada').append($('<option>', {
                        value: data[i].Clv_Motivo,
                        text: data[i].Descripcion
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
    });


    function tipoServicio(id) {
        $.ajax({
            url: "/CLIENTE/getTipoServicio/",
            type: "GET",
            data: { "IdPlaza": id },
            success: function (data, textStatus, jqXHR) {
                // una vez que fue exitosa la peticion asignamos los datos a un select
                for (var i = 0; i < data.length; i++) {
                    $('#tipo_servicio').append($('<option>', {
                        value: data[i].Clv_TipSer,
                        text: data[i].Concepto
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
    }

    function AbrirModal() {
        $('#Modal_Plaza').modal("show");
    }

    function regresar(){
        $('#tipo_cliente').val("0").change();
        $('#tipo_llamante').show();
        $('#regresar').hide();
        $('#btn_enviar').hide();
        $('#panel_no_cliente').hide();
        $('#panel_llamante').hide();
        $('#motivo_llamada_panel').hide();
        $('#queja').hide();
        $('#tipo_servicio').hide();
        $('#panel_fecha').hide();
        $('#panel_clasificacion').hide();
        $('#panel_solucion').hide();
        $('#panel_nueva_queja').hide();
        $('#motivo_no_cliente').hide();
        $('#solucion_no_cliente').hide();
        $('#telefono_no_cliente').hide();
        $('#domicilio_no_cliente').hide();
        $('#panel_tabla_clientes').hide();
        $('#panel_datos_cliente').hide();
        $('#email_no_cliente').hide();
        $('#panel_tipo_informacion').hide();
        $('#panel_tipo_llamada_cliente').hide();
        $('#motivo_llamada_cliente').hide();
        
        document.getElementById("llamadaForm").reset();
    }

    $('#tipo_llamada_cliente').on('change',function(){
        var tipo = $(this).val();
        if(tipo == 1){
            $('#panel_tipo_informacion').show();
            $('#motivo_llamada_panel').hide();
            $('#panel_clasificacion').hide();
            $('#solucion_no_cliente').hide();
            $('#panel_solucion').hide();
            $('#motivo_llamada_cliente').hide();
            $.ajax({
                url: "/Llamada/TipoLlamada/",
                type: "GET",
                success: function (data, textStatus, jqXHR) {
                    $('#tipo_informacion option').remove();
                    for (var i = 0; i < data.length; i++) {
                        $('#tipo_informacion').append($('<option>', {
                            value: data[i].clv_motivo,
                            text: data[i].descricpion
                        }));
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {

                }
            });
        }else if(tipo == 2){
            $('#panel_tipo_informacion').hide();
            $('#motivo_llamada_panel').show();
            $('#panel_clasificacion').show();
            $('#solucion_no_cliente').show();
            $('#panel_solucion').show();
            $('#motivo_llamada_cliente').hiden();
        }
    });

    function plaza_conexion(id) {
        $('#contenedor_llamda').hide();
        $('#llamdaForm').show();       
        $('#tipo_llamante').show();
        $('#panel_cliente').show();
        
        $.ajax({
            url: "/tblClasificacionProblema/GetClasficacionProblema/",
            type: "GET",
            data: { 'IdPlaza': id },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#select_problemas').append($('<option>', {
                        value: data[i].clvProblema,
                        text: data[i].descripcion
                    }));
                }
            },
            error: function (jqXHR, textStatus, errorThrown) {

            }
        });
        $.ajax({
            url: "/tblClasificacionProblema/GetDepartamentoResponsable/",
            type: "GET",
            data: { 'IdPlaza': id },
            success: function (data, textStatus, jqXHR) {
                for (var i = 0; i < data.length; i++) {
                    $('#departamento').append($('<option>', {
                        value: data[i].Clave,
                        text: data[i].Concepto
                    }));
